<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="srbyen.researchingapp" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Activities.Main"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".Activities.IntroducePin"
            android:label="@string/pin_activity_name"
            android:theme="@style/AppTheme">

        </activity>

        <activity
            android:name=".Activities.IntroducePass"
            android:label="@string/pass_activity_name"
            android:theme="@style/AppTheme">

        </activity>

        <activity
            android:name=".Activities.AdministratorOptions"
            android:label="@string/admin_activity_name"
            android:theme="@style/AppTheme">

        </activity>
        <activity android:name=".Activities.AdminPrivileges"
                  android:label="@string/app_name"/>

    </application>

</manifest>
